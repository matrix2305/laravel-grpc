{
    "name": "vandarpay/laravel-grpc",
    "description": "GRPC Laravel Package",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "vandarpay/service-repository": "^0.1",
        "ext-protobuf": "*",
        "ext-grpc": "*",
        "spiral/roadrunner": "^2.11",
        "spiral/roadrunner-laravel": "^5.9",
        "spiral/roadrunner-grpc": "^2.0",
        "nyholm/psr7": "^1.5",
        "google/common-protos": "^3.0",
        "grpc/grpc": "^1.36",
        "google/protobuf": "^3.11"
    },
    "authors": [
        {
            "name": "Hamid Mahmoudpour",
            "email": "mp.hamid@yahoo.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
              "LaravelGrpcServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "GrpcServices\\": "../../../app/Protobuf/GrpcServices",
            "vandarpay\\LaravelGrpc\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
